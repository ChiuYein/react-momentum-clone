ect.keys(localStorage).filter(key => {\n            key.includes(\"todo-item\")\n        });\n        const restoredLocal = localKeys.map(item => {\n            return JSON.parse(localStorage.getItem(item));\n        });\n\n        this.setState({\n            list: restoredLocal,\n            item: \"\"\n        });\n\n        event.preventDefault();\n    }\n\n    //Remove items\n    deleteItem(id) {\n        localStorage.removeItem(`todo-item-${id}`);\n        this.setState((prevState, { list }) => ({\n            list: prevState.list.filter(item => item.id !== id)\n        }));\n    }\n\n    activeHandler(){\n        this.setState({ active: !this.state.active });\n    }\n\n    render() {\n        console.log(\"list: \", this.state.list);\n        let activeToDoList = classNames({\n            \"todo-list-container\": true,\n            active: this.state.active\n        });\n\n        return(\n            <div className=\"todo-container\">\n                <div className={activeToDoList}>\n                    <TDList list={this.state.list} deleteItem={this.deleteItem} />\n                    <AddItem\n                      item={this.state.item}\n                      handleChange={this.handleChange}\n                      handleSubmit={this.handleSubmit}\n                    />\n                </div>\n                <div className=\"todo-click-label\">\n                    <span onClick={this.activeHandler}>To Do</span>\n                </div>\n            </div>\n        );\n    }\n}\n\nexport default ToDoList","/home/chiuyein/Desktop/sei/projects/project4_react_momentum_clone/momentum_clone/src/Components/ToDo/TDList.js",[],"/home/chiuyein/Desktop/sei/projects/project4_react_momentum_clone/momentum_clone/src/Components/ToDo/AddItem.js",[],"/home/chiuyein/Desktop/sei/projects/project4_react_momentum_clone/momentum_clone/src/Components/Quotes/Quotes.js",[],{"ruleId":"51","replacedBy":"52"},{"ruleId":"53","replacedBy":"54"},{"ruleId":"55","severity":1,"message"